{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "rohanADF"
		},
		"AzureBlobStorageLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageLS'"
		},
		"AzureSQLDatabaseLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSQLDatabaseLS'"
		},
		"AzureSqlEmptyDBLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlEmptyDBLS'"
		},
		"AzureSqlSampleDatabaseLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlSampleDatabaseLS'"
		},
		"FileServerLS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'FileServerLS'"
		},
		"SqlServerAdvWorksLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServerAdvWorksLS'"
		},
		"SqlServerAdvWorksLS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SqlServerAdvWorksLS'"
		},
		"SqlServerLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServerLS'"
		},
		"SqlServerLS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SqlServerLS'"
		},
		"FileServerLS_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\Users\\bridr\\Downloads\\"
		},
		"FileServerLS_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "LAPTOP-U93RR200\\bridr"
		},
		"SqlServerAdvWorksLS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "LAPTOP-U93RR200\\bridr"
		},
		"SqlServerLS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "LAPTOP-U93RR200\\bridr"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "rohanADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorageLS_connectionString": {
						"value": "[parameters('AzureBlobStorageLS_connectionString')]"
					},
					"AzureSQLDatabaseLS_connectionString": {
						"value": "[parameters('AzureSQLDatabaseLS_connectionString')]"
					},
					"AzureSqlEmptyDBLS_connectionString": {
						"value": "[parameters('AzureSqlEmptyDBLS_connectionString')]"
					},
					"AzureSqlSampleDatabaseLS_connectionString": {
						"value": "[parameters('AzureSqlSampleDatabaseLS_connectionString')]"
					},
					"FileServerLS_password": {
						"value": "[parameters('FileServerLS_password')]"
					},
					"SqlServerAdvWorksLS_connectionString": {
						"value": "[parameters('SqlServerAdvWorksLS_connectionString')]"
					},
					"SqlServerAdvWorksLS_password": {
						"value": "[parameters('SqlServerAdvWorksLS_password')]"
					},
					"SqlServerLS_connectionString": {
						"value": "[parameters('SqlServerLS_connectionString')]"
					},
					"SqlServerLS_password": {
						"value": "[parameters('SqlServerLS_password')]"
					},
					"FileServerLS_properties_typeProperties_host": {
						"value": "[parameters('FileServerLS_properties_typeProperties_host')]"
					},
					"FileServerLS_properties_typeProperties_userId": {
						"value": "[parameters('FileServerLS_properties_typeProperties_userId')]"
					},
					"SqlServerAdvWorksLS_properties_typeProperties_userName": {
						"value": "[parameters('SqlServerAdvWorksLS_properties_typeProperties_userName')]"
					},
					"SqlServerLS_properties_typeProperties_userName": {
						"value": "[parameters('SqlServerLS_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "rohanADF_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'rohanADF_ArmTemplate_0')]"
			]
		}
	]
}